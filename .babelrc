{
  "presets": [
      "react",
      "stage-1",
      "es2015"
    ],
    "env": {
      "development": {
        "presets": [
          "env"
        ]
      },
      "production": {
        "presets": [
          [
            "env",
            {
              "es2015": {
                "modules": false
              }
            }
          ]
        ],
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-remove-prop-types"
        ]
      },
      "test": {
        "presets": [
          "env"
        ]
      }
    }
}
